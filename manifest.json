{
  "manifest_version": 3,
  "name": "InNetwork Job Finder",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
   
    "tabs"
  ],


 
"host_permissions": [
    "*://*.linkedin.com/*"],


  "background": {
    "service_worker": "background.js"
  },


  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
